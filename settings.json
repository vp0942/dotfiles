{
  "files.autoSaveDelay": 500,
  "workbench.startupEditor": "none",
  "explorer.confirmDelete": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "debug.allowBreakpointsEverywhere": true,
  "rest-client.previewResponseInUntitledDocument": true,
  "editor.minimap.enabled": false,
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.fontSize": 17,
  // "editor.fontFamily": "'Fira Code'",
  // The Cursive font is operator Mono, it's $200 and you need to buy it to get the cursive. Dank Mono or Victor Mono are good alternatives ,
  // "editor.fontFamily": "'Inconsolata for Powerline','Operator Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontFamily": "'Inconsolata NFM','Operator Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontWeight": "400", // Regular
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": true
  },
  // Wes Bos Cobalt 2
  "workbench.colorTheme": "Cobalt2",
  "editor.lineHeight": 25,
  "editor.letterSpacing": 0.5,
  "files.trimTrailingWhitespace": true,
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 1,
  "editor.cursorBlinking": "solid",
  "editor.renderWhitespace": "none",
  "css.fileExtensions": [
    "css",
    "scss"
  ],
  "explorer.confirmDragAndDrop": false,
  "security.workspace.trust.untrustedFiles": "open",
  //cmder setup
  // "terminal.external.windowsExec": "D:\\Portable\\cmder\\cmder.exe",
  // "terminal.explorerKind": "external",
  // "terminal.integrated.profiles.windows": {
